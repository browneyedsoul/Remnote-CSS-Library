{
  "name": "remnote-plugins",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check-types": "tsc",
    "sass": "pnpm run -r sass",
    "recall": "pnpm --filter active-recall dev",
    "recall-sass": "pnpm --filter active-recall sass",
    "recall:build": "pnpm --filter active-recall build",
    "recall:sass": "concurrently \"pnpm --filter active-recall sass\" \"pnpm --filter active-recall dev\"",
    "cloud": "pnpm --filter alias-cloud dev",
    "cloud-sass": "pnpm --filter alias-cloud sass",
    "cloud:build": "pnpm --filter alias-cloud build",
    "cloud:sass": "concurrently \"pnpm --filter alias-cloud sass\" \"pnpm --filter alias-cloud dev\"",
    "article": "pnpm --filter article-comment dev",
    "article-sass": "pnpm --filter article-comment sass",
    "article:build": "pnpm --filter article-comment build",
    "article:sass": "concurrently \"pnpm --filter article-comment sass\" \"pnpm --filter article-comment dev\"",
    "aligner": "pnpm --filter aligner dev",
    "aligner-sass": "pnpm --filter aligner sass",
    "aligner:build": "pnpm --filter aligner build",
    "aligner:sass": "concurrently \"pnpm --filter aligner sass\" \"pnpm --filter aligner dev\"",
    "callout": "pnpm --filter callout dev",
    "callout-sass": "pnpm --filter callout sass",
    "callout:build": "pnpm --filter callout build",
    "callout:sass": "concurrently \"pnpm --filter callout sass\" \"pnpm --filter callout dev\"",
    "button": "pnpm --filter colorful-answer-button dev",
    "button-sass": "pnpm --filter colorful-answer-button sass",
    "button:build": "pnpm --filter colorful-answer-button build",
    "button:sass": "concurrently \"pnpm --filter colorful-answer-button sass\" \"pnpm --filter colorful-answer-button dev\"",
    "divider": "pnpm --filter divider dev",
    "divider-sass": "pnpm --filter divider sass",
    "divider:build": "pnpm --filter divider build",
    "divider:sass": "concurrently \"pnpm --filter divider sass\" \"pnpm --filter divider dev\"",
    "enforcer": "pnpm --filter image-size-enforcer dev",
    "enforcer-sass": "pnpm --filter image-size-enforcer sass",
    "enforcer:build": "pnpm --filter image-size-enforcer build",
    "enforcer:sass": "concurrently \"pnpm --filter image-size-enforcer sass\" \"pnpm --filter image-size-enforcer dev\"",
    "kanban": "pnpm --filter kanban dev",
    "kanban-sass": "pnpm --filter kanban sass",
    "kanban:build": "pnpm --filter kanban build",
    "kanban:sass": "concurrently \"pnpm --filter kanban sass\" \"pnpm --filter kanban dev\"",
    "table": "pnpm --filter minimal-table dev",
    "table-sass": "pnpm --filter minimal-table sass",
    "table:build": "pnpm --filter minimal-table build",
    "table:sass": "concurrently \"pnpm --filter minimal-table sass\" \"pnpm --filter minimal-table dev\"",
    "nobullet": "pnpm --filter no-bullet-editor-mode dev",
    "nobullet-sass": "pnpm --filter no-bullet-editor-mode sass",
    "nobullet:build": "pnpm --filter no-bullet-editor-mode build",
    "nobullet:sass": "concurrently \"pnpm --filter no-bullet-editor-mode sass\" \"pnpm --filter no-bullet-editor-mode dev\"",
    "card": "pnpm --filter rem-card-icon dev",
    "card-sass": "pnpm --filter rem-card-icon sass",
    "card:build": "pnpm --filter rem-card-icon build",
    "card:sass": "concurrently \"pnpm --filter rem-card-icon sass\" \"pnpm --filter rem-card-icon dev\"",
    "tree": "pnpm --filter rem-tree dev",
    "tree-sass": "pnpm --filter rem-tree sass",
    "tree:build": "pnpm --filter rem-tree build",
    "tree:sass": "concurrently \"pnpm --filter rem-tree sass\" \"pnpm --filter rem-tree dev\"",
    "runjs": "pnpm --filter run-js dev",
    "runjs-sass": "pnpm --filter run-js sass",
    "runjs:build": "pnpm --filter run-js build",
    "runjs:sass": "concurrently \"pnpm --filter run-js sass\" \"pnpm --filter run-js dev\"",
    "sidebar": "pnpm --filter sidebar-enhancer dev",
    "sidebar-sass": "pnpm --filter sidebar-enhancer sass",
    "sidebar:build": "pnpm --filter sidebar-enhancer build",
    "sidebar:sass": "concurrently \"pnpm --filter sidebar-enhancer sass\" \"pnpm --filter sidebar-enhancer dev\"",
    "colorizer": "pnpm --filter text-colorizer dev",
    "colorizer-sass": "pnpm --filter text-colorizer sass",
    "colorizer:build": "pnpm --filter text-colorizer build",
    "colorizer:sass": "concurrently \"pnpm --filter text-colorizer sass\" \"pnpm --filter text-colorizer dev\"",
    "vertico": "pnpm --filter remacs-vertico dev",
    "vertico-sass": "pnpm --filter remacs-vertico sass",
    "vertico:build": "pnpm --filter remacs-vertico build",
    "vertico:sass": "concurrently \"pnpm --filter remacs-vertico sass\" \"pnpm --filter remacs-vertico dev\""
  },
  "dependencies": {
    "@remnote/plugin-sdk": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/node": "^18.19.69",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "autoprefixer": "^10.4.20",
    "bestzip": "^2.2.1",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "esbuild-loader": "^2.21.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.3.4",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.14.2",
    "sass": "^1.83.1",
    "sass-loader": "^14.2.1",
    "shx": "^0.3.4",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@remnote/plugin-sdk@0.0.46": "patches/@remnote__plugin-sdk@0.0.46.patch"
    }
  }
}
